gcc -g -c -Wall -o main.o main.c
gcc -g -Wall -lpthread -ldl -o server locks.o Application.o fileHandler.o Prompt.o genlib.o pqADT.o scannerADT.o tree.o Session.o sqlite.o sqliteADT.o main.o DBexec.o Server.o Transport.o ipcFifo.o CallApp.o OutputPipe.o
